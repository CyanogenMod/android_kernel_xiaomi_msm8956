						reg = <1>;
						qcom,psci-cpu-mode = <2>;
						qcom,spm-cpu-mode = "retention";
						qcom,latency-us = <99>;
						qcom,ss-power = <545>;
						qcom,energy-overhead = <150122>;
						qcom,time-overhead = <239>;
