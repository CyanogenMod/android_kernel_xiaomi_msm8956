						qcom,spm-cpu-mode = "wfi";
						qcom,latency-us = <58>;
						qcom,ss-power = <547>;
						qcom,energy-overhead = <61640>;
						qcom,time-overhead = <118>;
					};

					qcom,pm-cpu-level@1 {
						reg = <1>;
